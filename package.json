{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "preinstall": "echo 'Skipping Node version check for preview build'",
    "dev": "vite",
    "prebuild:legal": "node scripts/generate-legal-manifest.mjs",
    "prebuild": "npm run prebuild:legal",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "preview:static": "vite preview --port 4173 --host",
    "lint": "eslint .",
    "lint:check": "eslint . --max-warnings 0",
    "preview": "vite preview",
    "pdf2md": "bash scripts/pdf2md.sh",
    "env:doctor": "ts-node --esm scripts/envDoctor.ts",
    "env:doctor:strict": "ts-node --esm scripts/envDoctor.ts --strict",
    "setup:env": "ts-node --esm scripts/bootstrap-env.ts",
    "secrets:set": "ts-node --esm scripts/set-secrets.ts",
    "dev:seed": "TS_NODE_PROJECT=backend/tsconfig.json ts-node --esm scripts/seed-dev.ts",
    "preflight": "ts-node --esm scripts/preflight.ts",
    "first-run": "./scripts/first-run.sh",
    "dev:all": "./scripts/dev.sh",
    "predev:all": "npm run env:doctor",
    "postman:open": "echo 'üìÆ Postman Collection: tooling/postman/PBCEx.postman_collection.json' && echo 'üåç Local Environment: tooling/postman/PBCEx.local.postman_environment.json' && echo 'üè¢ Staging Environment: tooling/postman/PBCEx.staging.postman_environment.json'",
    "thunder:open": "echo '‚ö° Thunder Collection: tooling/thunder/thunder-collection_PBCEx.json' && echo 'üåç Local Environment: tooling/thunder/thunder-environment_local.json' && echo 'üè¢ Staging Environment: tooling/thunder-environment_staging.json'",
    "cap:init": "node -e \"console.log('capacitor.config.ts')\"",
    "cap:add:ios": "echo '‚ö†Ô∏è  Run locally only; do not commit ios/ platform outputs' && npx cap add ios",
    "ios:patch-entitlements": "ts-node --esm scripts/ios/patch-entitlements.ts",
    "commit": "git-cz",
    "prepare": "husky install",
    "test": "echo 'No frontend tests configured yet'",
    "type-check": "tsc --noEmit",
    "e2e:staging": "cd e2e && BASE_URL=$STAGING_WEB_BASE_URL npx playwright test --config playwright.config.ts",
    "e2e:smoke": "cd e2e && npx playwright test --config playwright.config.ts --project=chromium --workers=1 --grep @smoke",
    "smoke:staging": "cd e2e && BASE_URL=$STAGING_WEB_BASE_URL npx playwright test --config playwright.config.ts --project=chromium --workers=1 --grep @smoke --reporter=html",
    "e2e:report": "cd e2e && npx playwright show-report test-results/html-report",
    "prepush": "npx gitleaks detect --verbose --no-git || exit 0"
  },
